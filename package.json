{
  "name": "hexoplusplus",
  "version": "2.0.1-beta-9",
  "description": "A Serverless And Modern Hexo Backend DashBoard",
  "main": "worker/kernel.js",
  "CDN": "https://cdn.jsdelivr.net/npm/hexoplusplus/",
  "scripts": {
    "build:worker": "webpack -c webpack.worker.js",
    "build:static": "webpack -c webpack.static.js",
    "build:worker:dev": "webpack -c webpack.worker.js --watch",
    "build:static:dev": "webpack -c webpack.static.js --watch",
    "server": "http-server -S -C ./ssl/cert.pem -K ./ssl/key.pem ./dist/ -p 9999",
    "server:nossl": "http-server ./dist/ -p 9999",
    "build": "npm run build:worker && npm run build:static",
    "build:server": "npm run build && npm run server",
    "build:server:nossl": "npm run build && npm run server:nossl"
  },
  "author": "ChenYFan",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HexoPlusPlus/HexoPlusPlus.git"
  },
  "keywords": [
    "Hexo",
    "BackDashboard"
  ],
  "bugs": {
    "url": "https://github.com/HexoPlusPlus/HexoPlusPlus/issues"
  },
  "homepage": "https://github.com/HexoPlusPlus/HexoPlusPlus#readme",
  "dependencies": {
    "html-loader": "^2.1.2",
    "js-yaml": "^4.1.0",
    "md5": "^2.3.0",
    "raw-loader": "^4.0.2",
    "style-loader": "^3.0.0",
    "webpack": "^5.30.0",
    "webpack-cli": "^4.7.0"
  },
  "devDependencies": {
    "cssnano": "^5.0.4",
    "optimize-css-assets-webpack-plugin": "^6.0.0"
  }
}
